{
  "extends": [
    "airbnb-base",
    "airbnb-typescript/base",
    "plugin:prettier/recommended"
  ],
  "parserOptions": {
    "project": "./tsconfig.json",
    "tsconfigRootDir": "./"
  },
  "plugins": ["prettier"],
  "rules": {
    "no-console": "off",
    "eslint-plugin-import/prefer-default-export": "off",
  },
  "ignorePatterns": ["/node_modules/", "/dist/"],
  "overrides": []
}
